    "@testing-library/react": "^9.4.0",
    "apollo-boost": "^0.4.7",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "qs": "^6.9.1",
